{
  "name": "bocalc",
  "version": "0.1.0",
  "private": true,
  "description": "Back Office Calculator for car shipping from USA - multi-vendor platform",
  "author": "Kirill Za <zaaatakyrylo@gmail.com>",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "pages:build": "npx @cloudflare/next-on-pages",
    "pages:deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static --project-name=bocalc",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "worker:dev": "wrangler dev --local",
    "worker:deploy": "wrangler deploy",
    "db:create": "wrangler d1 create bocalc-db",
    "db:migrate": "wrangler d1 migrations apply bocalc-db",
    "db:migrate:local": "wrangler d1 migrations apply bocalc-db --local",
    "db:console": "wrangler d1 execute bocalc-db",
    "sheets:setup": "node scripts/setup-google-sheets.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.1.5",
    "@tanstack/react-query": "^5.17.19",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.0.6",
    "googleapis": "^131.0.0",
    "hono": "^3.12.8",
    "jose": "^5.2.0",
    "lucide-react": "^0.309.0",
    "next": "^15.5.2",
    "next-intl": "^3.4.5",
    "next-themes": "^0.2.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.49.3",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.16",
    "@cloudflare/workers-types": "^4.20240117.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.8",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^29.7.0",
    "postcss": "^8.4.33",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.10",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vercel": "^48.10.3",
    "wrangler": "^3.24.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
